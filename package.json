{
  "name": "winston-console-transport",
  "version": "0.0.5",
  "description": "provides a console transport for winston logger",
  "main": "./lib",
  "config": {
    "MOCHA_OPTS": "--timeout 60000 --exit --reporter spec --ui qunit tests/*.js"
  },
  "scripts": {
    "build": "babel src --out-dir ./lib",
    "prepare": "npm run build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "npm run test:lint && npm run test:security && npm run test:mocha",
    "test:mocha": "mocha  --config .mocharc.json tests/**/*.js",
    "test:coverage": "BUILD=0 MODE=test nyc mocha --config .mocharc.json tests/**/*.js",
    "test:lint": "eslint src --ext js",
    "test:security": "npm audit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pustovitDmytro/winston-console-transport.git"
  },
  "author": "Dmytro Pustovit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pustovitDmytro/winston-console-transport/issues"
  },
  "keywords": [
    "winston",
    "console",
    "transport",
    "logger",
    "native",
    "verbose",
    "log",
    "debug",
    "browser"
  ],
  "homepage": "https://github.com/pustovitDmytro/winston-console-transport#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.1",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.2.0",
    "eslint": "^5.15.1",
    "eslint-config-webbylab": "^4.0.0",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-more": "^0.3.2",
    "eslint-plugin-no-require-lodash": "^1.1.0",
    "eslint-plugin-prefer-spread": "^1.0.3",
    "eslint-plugin-react": "^7.12.4",
    "fs-extra": "^8.1.0",
    "husky": "^6.0.0",
    "mocha": "^8.3.2",
    "nyc": "^14.1.1",
    "semantic-release": "^17.4.2",
    "test-console": "^1.1.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "winston-transport": "^4.3.0"
  }
}
